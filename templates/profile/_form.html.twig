<div class="container">
  <div class="stepwizard">
    <div class="stepwizard-row setup-panel">
      <div class="stepwizard-step">
        <a href="#step-1" type="button" class="btn btn-primary btn-circle">
          <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-building" viewBox="0 0 16 16">
            <path fill-rule="evenodd" d="M14.763.075A.5.5 0 0 1 15 .5v15a.5.5 0 0 1-.5.5h-3a.5.5 0 0 1-.5-.5V14h-1v1.5a.5.5 0 0 1-.5.5h-9a.5.5 0 0 1-.5-.5V10a.5.5 0 0 1 .342-.474L6 7.64V4.5a.5.5 0 0 1 .276-.447l8-4a.5.5 0 0 1 .487.022zM6 8.694 1 10.36V15h5V8.694zM7 15h2v-1.5a.5.5 0 0 1 .5-.5h2a.5.5 0 0 1 .5.5V15h2V1.309l-7 3.5V15z"/>
            <path d="M2 11h1v1H2v-1zm2 0h1v1H4v-1zm-2 2h1v1H2v-1zm2 0h1v1H4v-1zm4-4h1v1H8V9zm2 0h1v1h-1V9zm-2 2h1v1H8v-1zm2 0h1v1h-1v-1zm2-2h1v1h-1V9zm0 2h1v1h-1v-1zM8 7h1v1H8V7zm2 0h1v1h-1V7zm2 0h1v1h-1V7zM8 5h1v1H8V5zm2 0h1v1h-1V5zm2 0h1v1h-1V5zm0-2h1v1h-1V3z"/>
          </svg>
        </a>
        <p>Identification</p>
      </div>
      <div class="stepwizard-step">
        <a href="#step-2" type="button" class="btn btn-default btn-circle" disabled="disabled">
          <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-geo-alt" viewBox="0 0 16 16">
            <path d="M12.166 8.94c-.524 1.062-1.234 2.12-1.96 3.07A31.493 31.493 0 0 1 8 14.58a31.481 31.481 0 0 1-2.206-2.57c-.726-.95-1.436-2.008-1.96-3.07C3.304 7.867 3 6.862 3 6a5 5 0 0 1 10 0c0 .862-.305 1.867-.834 2.94zM8 16s6-5.686 6-10A6 6 0 0 0 2 6c0 4.314 6 10 6 10z"/>
            <path d="M8 8a2 2 0 1 1 0-4 2 2 0 0 1 0 4zm0 1a3 3 0 1 0 0-6 3 3 0 0 0 0 6z"/>
          </svg>
        </a>
        <p>Statut</p>
      </div>
      <div class="stepwizard-step">
        <a href="#step-3" type="button" class="btn btn-default btn-circle" disabled="disabled">
          <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-person-plus" viewBox="0 0 16 16">
            <path d="M6 8a3 3 0 1 0 0-6 3 3 0 0 0 0 6zm2-3a2 2 0 1 1-4 0 2 2 0 0 1 4 0zm4 8c0 1-1 1-1 1H1s-1 0-1-1 1-4 6-4 6 3 6 4zm-1-.004c-.001-.246-.154-.986-.832-1.664C9.516 10.68 8.289 10 6 10c-2.29 0-3.516.68-4.168 1.332-.678.678-.83 1.418-.832 1.664h10z"/>
            <path fill-rule="evenodd" d="M13.5 5a.5.5 0 0 1 .5.5V7h1.5a.5.5 0 0 1 0 1H14v1.5a.5.5 0 0 1-1 0V8h-1.5a.5.5 0 0 1 0-1H13V5.5a.5.5 0 0 1 .5-.5z"/>
          </svg>
        </a>
        <p>Cv</p>
      </div>
      <div class="stepwizard-step">
        <a href="#step-4" type="button" class="btn btn-default btn-circle" disabled="disabled">
          <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-geo-alt" viewBox="0 0 16 16">
            <path d="M12.166 8.94c-.524 1.062-1.234 2.12-1.96 3.07A31.493 31.493 0 0 1 8 14.58a31.481 31.481 0 0 1-2.206-2.57c-.726-.95-1.436-2.008-1.96-3.07C3.304 7.867 3 6.862 3 6a5 5 0 0 1 10 0c0 .862-.305 1.867-.834 2.94zM8 16s6-5.686 6-10A6 6 0 0 0 2 6c0 4.314 6 10 6 10z"/>
            <path d="M8 8a2 2 0 1 1 0-4 2 2 0 0 1 0 4zm0 1a3 3 0 1 0 0-6 3 3 0 0 0 0 6z"/>
          </svg>
        </a>
        <p>Adresse</p>
      </div>
    </div>
  </div>
  {{ form_start(form) }}
  <div class="row setup-content" id="step-1">
    <div class="col-xs-12">
      <div class="col-md-12">
        {{ form_row(form.civilite) }}
        {{ form_row(form.description) }}
        {{ form_row(form.birthdate) }}
        {{ form_row(form.diplome) }}
        {{ form_row(form.experiences) }}
        {{ form_row(form.metiers) }}
        {{ form_row(form.zoneDeRecherche) }}
        {{ form_row(form.portfolio) }}
        <div class="btn-toolbar mt-4">
          <button type="button" class="btn app-btn-primary btn-sm nextBtn pull-right"><i class="bi bi-arrow-right-short"></i> Suivant</button>
        </div>
      </div>
    </div>
  </div>
  <div class="row setup-content" id="step-2">
    <div class="col-xs-12">
      <div class="col-md-12">
        {{ form_row(form.isRqth) }}
        {{ form_row(form.isVisible) }}
        {{ form_row(form.isAmenagement) }}
        <div class="btn-toolbar mt-4">
          <button type="button" class="btn app-btn-light btn-sm nextBtn pull-left"><i class="bi bi-arrow-left-short"></i> Précédent</button>
          <button type="button" class="btn app-btn-primary btn-sm nextBtn pull-right"><i class="bi bi-arrow-right-short"></i> Suivant</button>
        </div>
      </div>
    </div>
  </div>
  <div class="row setup-content" id="step-3">
    <div class="col-xs-12">
      <div class="col-md-12">
        {{ form_row(form.cv) }}
        <div class="btn-toolbar mt-4">
          <button type="button" class="btn app-btn-light btn-sm nextBtn pull-left"><i class="bi bi-arrow-left-short"></i> Précédent</button>
          <button type="button" class="btn app-btn-primary btn-sm nextBtn pull-right"><i class="bi bi-arrow-right-short"></i> Suivant</button>
        </div>
      </div>
    </div>
  </div>
  <div class="row setup-content" id="step-4">
    <div class="col-xs-12">
      <div class="col-md-12">
        <div id="adresse" data-prototype="{{ form_row(form.adresse.vars.prototype)|e('html_attr') }}">
          {{ form_row(form.adresse) }}
          <span></span>
        </div>
        <div class="btn-toolbar mt-4">
          <button type="button" class="btn app-btn-light btn-sm nextBtn pull-left"><i class="bi bi-arrow-left-short"></i> Précédent</button>
          <button type="button" class="btn app-btn-primary btn-sm nextBtn pull-right"><i class="bi bi-arrow-right-short"></i> Suivant</button>
        </div>
      </div>
    </div>
  </div>
  <input type="submit" value="Enregistrer" class="bouton solid"/>
  {{ form_end(form) }}
</div>

<style>
    #profile_birthdate_time_hour{
        display: none;
    }
    div.sr-only{
        display: none;
    }
</style>
