<div class="col-md-4 col-sm-6 text-center" id="entreprise-{{ entreprise.id }}">
    <div class="box">
        <div class="avatar">
            <img class="img-fluid" src="{{ asset(entreprise.getUrlLastLogo()) }}" alt="{{ entreprise.getNameLastLogo() }}" />
        </div>
        <div class="box_content">
            <h3 class="title"><a href="{{ path('entreprise_show_unit', {id:entreprise.id, slug:entreprise.slug}) }}">{{ entreprise.name }}</a></h3>
            <span class="post">
                {% if entreprise.annoncesEntreprise|length >= 1 %}
                    <span class="">{{ pluralize(entreprise.annoncesEntreprise|length, 'annonce') }}</span>
                {% endif %}
            </span>
        </div>
      {#  <ul class="icons">
            <li><a href=""><i class="bi bi-facebook"></i></a></li>
            <li><a href=""><i class="bi bi-facebook"></i></a></li>
            <li><a href=""><i class="bi bi-facebook"></i></a></li>
        </ul>#}
    </div>
</div>
