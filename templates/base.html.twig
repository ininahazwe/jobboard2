<!DOCTYPE html>
<html lang="{{ app.request.locale }}">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
        <title>{% block title %}Hanci CV - Recrutement de personnes en situation d'handicap{% endblock %}</title>
        <meta charset="utf-8">
        <meta content="" name="description">
        <meta content="" name="keywords">
        <meta name="turbo-visit-control" content="reload">

        <!-- Favicons -->
        <link href="{{ asset('assets/img/favicon.png') }}" rel="icon">
        <link href="{{ asset('assets/img/apple-touch-icon.png') }}" rel="apple-touch-icon">

        <!-- Google Fonts -->
        <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Jost:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">
        <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/fontawesome.min.css">
        {{ encore_entry_link_tags('app') }}
        {% include 'layouts/css.html.twig' %}
        {% include 'partials/scripts.html.twig' %}
        {{ encore_entry_script_tags('app') }}

        {% block stylesheets %}

        {% endblock %}
        {% block javascripts %}

        {% endblock %}
    </head>
    <body data-turbo="false">
        {{ render(controller('App\\Controller\\HomeController::menuIndex')) }}
        {{ include('partials/messages.html.twig') }}
        {% block body %}
        {% endblock %}
        {{ render(controller('App\\Controller\\HomeController::footerIndex')) }}
        {# {{ render(controller('App\\Controller\\HomeController::bodyIndex')) }} #}
    </body>
</html>

