<form method="post" action="{{ path('candidature_delete', {'id': candidature.id}) }}" onsubmit="return confirm('êtes-vous sûr de vouloir supprimer cette candidature ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ candidature.id) }}">
    <button class="btn"><i class="fas fa-trash"></i></button>
</form>
